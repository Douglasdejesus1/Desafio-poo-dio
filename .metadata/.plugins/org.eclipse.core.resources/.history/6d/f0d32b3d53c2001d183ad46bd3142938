package domain.model;

public class AtividadeCurso extends Treinamento {
	
	private int cargaHoraria;

	public AtividadeCurso(int cargaHoraria) {
		super();
		this.cargaHoraria = cargaHoraria;
	}
	public AtividadeCurso( ) {
	 	}

	public int getCargaHoraria() {
		return cargaHoraria;
	}

	public void setCargaHoraria(int cargaHoraria) {
		this.cargaHoraria = cargaHoraria;
	}

	@Override
	public String toString() {
		return "AtividadeCurso [cargaHoraria=" + cargaHoraria + ", getTitulo()=" + getTitulo() + ", getDescricao()="
				+ getDescricao() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + ", toString()="
				+ super.toString() + "]";
	}

	
	
}
